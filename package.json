{
    "name": "@nuejs/nue",
    "version": "0.19.0",
    "description": "Next Generation Reactive JavaScript Framework",
    "main": "dist/nue.cjs.js",
    "module": "dist/nue.es.js",
    "unpkg": "dist/nue.umd.prod.js",
    "jsdelivr": "dist/nue.umd.prod.js",
    "scripts": {
        "test": "jest",
        "bundle:cjsProd": "rollup -c --bundle cjsProd",
        "bundle:cjsDev": "rollup -c --bundle cjsDev --dev",
        "bundle:umdProd": "rollup -c --bundle umdProd",
        "bundle:umdDev": "rollup -c --bundle umdDev --dev",
        "bundle:es": "rollup -c --bundle es --dev",
        "bundle:cjs": "npm run bundle:cjsProd && npm run bundle:cjsDev",
        "bundle:umd": "npm run bundle:umdProd && npm run bundle:umdDev",
        "bundle": "npm run bundle:cjs && npm run bundle:umd && npm run bundle:es"
    },
    "keywords": [
        "nuejs",
        "nue",
        "ui library",
        "framework",
        "reactive",
        "web components"
    ],
    "author": "Manan Tank",
    "license": "MIT",
    "devDependencies": {
        "@babel/core": "^7.12.9",
        "@babel/plugin-transform-modules-commonjs": "^7.12.1",
        "@types/jest": "^26.0.15",
        "babel-jest": "^26.6.3",
        "eslint": "^7.14.0",
        "eslint-config-standard": "^16.0.2",
        "eslint-plugin-import": "^2.22.1",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-promise": "^4.2.1",
        "jest": "^26.6.3",
        "rollup": "^2.43.0",
        "rollup-plugin-replace": "^2.2.0",
        "rollup-plugin-terser": "^7.0.2"
    },
    "dependencies": {},
    "sideEffects": false
}
